    "ember-source": "~3.1.1",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "0.2.23",
    "eslint-plugin-ember": "5.1.0",
    "eslint-plugin-node": "6.0.1",
    "loader.js": "4.7.0"
