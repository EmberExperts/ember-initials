    "ember-cli-app-version": "3.1.3",
    "ember-cli-code-coverage": "0.4.2",
    "ember-cli-dependency-checker": "2.1.0",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-fastboot": "1.1.3",
    "ember-cli-favicon": "1.0.0-beta.4",
    "ember-cli-github-pages": "0.2.0",
